<Skin Name="Default" Version="0.7">
  <Info>
    <Name>Neoforce Default</Name>
    <Description>Default skin for Neoforce Controls.></Description>
    <Author>Tom Shane</Author>
    <Version>0.7></Version>
  </Info>  

  <Controls>    

    //// Control ///////////////////////////////////////////////////////////////
    <Control Name="Control">
      <OriginMargins Left="0" Top="0" Right="0" Bottom="0"/>
      <ClientMargins Left="0" Top="0" Right="0" Bottom="0"/>      
      <DefaultSize Width="64" Height="64"/>
      <MinimumSize Width="0" Height="0"/>
      <ResizerSize Value="4"/>
      <Layers>
        <Layer Name="Control" Image="Control" Width="1" Height="1" Alignment="MiddleLeft" OffsetX="0" OffsetY="0" >
          <SizingMargins Left="0" Top="0" Right="0" Bottom="0"/>
          <ContentMatgins Left="0" Top="0" Right="0" Bottom="0"/>
          <States>
            <Enabled Index="0" Color="255;255;255;255" Overlay="false"/>
            <Hovered Index="0" Color="255;255;255;255" Overlay="false"/>
            <Pressed Index="0" Color="255;255;255;255" Overlay="false"/>
            <Focused Index="0" Color="255;255;255;255" Overlay="false"/>
            <Disabled Index="0" Color="255;255;255;255" Overlay="false"/>
          </States>
          <Overlays>
            <Enabled Index="0" Color="255;255;255;255"/>
            <Hovered Index="0" Color="255;255;255;255"/>
            <Pressed Index="0" Color="255;255;255;255"/>
            <Focused Index="0" Color="255;255;255;255"/>
            <Disabled Index="0" Color="255;255;255;255"/>
          </Overlays>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="0;0;0;255"/>
              <Focused Color="0;0;0;255"/>
              <Pressed Color="0;0;0;255"/>
              <Hovered Color="0;0;0;255"/>
              <Disabled Color="0;0;0;255"/>
            </Colors>
          </Text>
        </Layer>       
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// Control.Outline///////////////////////////////////////////////////////
    <Control Name="Control.Outline">
      <Layers>
        <Layer Name="Control" Image="Control.Outline" Width="1" Height="1">
          <States>
            <Enabled Index="0" Color="96;96;96;200"/>
          </States>  
        </Layer>
      </Layers>
    </Control>
    ///////////////////////////////////////////////////////////////////////////

    //// Button ////////////////////////////////////////////////////////////////
    <Control Name="Button" Inherits="Control">
      <DefaultSize Width="72" Height="24"/>      
      <Layers>
        <Layer Name="Control" Image="Button" Width="64" Height="24">
          <SizingMargins Left="2" Top="2" Right="2" Bottom="2"/>
          <ContentMargins Left="4" Top="4" Right="4" Bottom="4"/>
          <States>
            <Enabled Index="0" Color="255;255;255;255" Overlay="false"/>
            <Hovered Index="0" Color="255;255;255;255" Overlay="true"/>
            <Pressed Index="2" Color="255;255;255;255" Overlay="true"/>
            <Focused Index="0" Color="255;255;255;255" Overlay="true"/>
            <Disabled Index="4" Color="255;255;255;255" Overlay="false"/>
          </States>
          <Overlays>
            <Hovered Index="1" Color="255;255;255;255"/>
            <Pressed Index="3" Color="255;255;255;255"/>
            <Focused Index="5" Color="255;255;255;255"/>
          </Overlays>
          <Text Font="Default" Alignment="MiddleCenter" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;192"/>
              <Focused Color="255;255;255;192"/>
              <Pressed Color="255;255;255;192"/>
              <Hovered Color="255;255;255;240"/>
              <Disabled Color="128;128;128;192"/>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// CheckBox //////////////////////////////////////////////////////////////
    <Control Name="CheckBox" Inherits="Button">
      <Layers>
        <Layer Name="Control" Image="CheckBox" Width="13" Height="13">
          <ContentMargins Left="0" Top="0" Right="0" Bottom="0"/>
          <SizingMargins Left="2" Top="2" Right="2" Bottom="2"/>
          <States>
            <Enabled Index="0" Overlay="false"/>
            <Hovered Index="1" Overlay="false"/>
            <Pressed Index="2" Overlay="false"/>
            <Focused Index="3" Overlay="false"/>
            <Disabled Index="4" Overlay="false"/>
          </States>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;192"/>
              <Focused Color="255;255;255;192"/>
              <Pressed Color="255;255;255;192"/>
              <Hovered Color="255;255;255;240"/>
              <Disabled Color="255;255;255;32"/>
            </Colors>
          </Text>
        </Layer>
        <Layer Name="Checked" Image="CheckBox.Checked" Width="13" Height="13">
          <ContentMargins Left="0" Top="0" Right="0" Bottom="0"/>
          <SizingMargins Left="2" Top="2" Right="2" Bottom="2"/>
          <States>
            <Enabled Index="0"></Enabled>
            <Hovered Index="1"></Hovered>
            <Pressed Index="2"></Pressed>
            <Focused Index="3"></Focused>
            <Disabled Index="4"></Disabled>
          </States>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;192"/>
              <Focused Color="255;255;255;192"/>
              <Pressed Color="255;255;255;192"/>
              <Hovered Color="255;255;255;240"/>
              <Disabled Color="255;255;255;32"/>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// RadioButton ///////////////////////////////////////////////////////////
    <Control Name="RadioButton" Inherits="CheckBox">
      <Layers>
        <Layer Name="Control" Image="RadioButton" Width="13" Height="13">
        </Layer>
        <Layer Name="Checked" Image="RadioButton.Checked" Width="13" Height="13">
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// Window ////////////////////////////////////////////////////////////////
    <Control Name="Window" Inherits="Control">
      <DefaultSize Width="480" Height="360"/>
      <MinimumSize Width="100" Height="75"/>
      <ResizerSize Value="7"/>      
      <ClientMargins Left="7" Top="7" Right="7" Bottom="7"/>
      <Layers>
        <Layer Name="Control" Override="true" Image="Window" Width="16" Height="16">
          <States>
            <Enabled Index="0" Color="255;255;255;255"/>
          </States>
        </Layer>
        <Layer Name="Caption" Image="Window.Caption" Width="16" Height="28">
          <SizingMargins Left="6" Top="2" Right="6" Bottom="2" />
          <ContentMargins Left="6" Top="6" Right="6" Bottom="6" />
          <States>
            <Enabled Index="1" />
            <Focused Index="0" />
            <Disabled Index="1" />
          </States>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;192" />
              <Focused Color="255;255;255;240" />
              <Disabled Color="128;128;128;192" />
            </Colors>
          </Text>
        </Layer>
        <Layer Name="FrameTop" Image="Window.FrameTop" Width="16" Height="7">
          <SizingMargins Left="6" Top="2" Right="6" Bottom="2"/>
          <ContentMargins Left="6" Top="6" Right="6" Bottom="6"/>
          <States>
            <Enabled Index="1"/>
            <Focused Index="0"/>
            <Disabled Index="1"/>
          </States>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;192"/>
              <Focused Color="255;255;255;240"/>
              <Disabled Color="128;128;128;192"/>
            </Colors>
          </Text>
        </Layer>
        <Layer Name="FrameLeft" Image="Window.FrameLeft" Width="7" Height="16">
          <SizingMargins Left="2" Top="2" Right="2" Bottom="2"/>
          <States>
            <Enabled Index="1"/>
            <Focused Index="0"/>
            <Disabled Index="1"/>
          </States>
        </Layer>
        <Layer Name="FrameRight" Image="Window.FrameRight" Width="7" Height="16">
          <SizingMargins Left="2" Top="2" Right="2" Bottom="2"/>
          <States>
            <Enabled Index="1"/>
            <Focused Index="0"/>
            <Disabled Index="1"/>
          </States>
        </Layer>
        <Layer Name="FrameBottom" Image="Window.FrameBottom" Width="16" Height="7">
          <SizingMargins Left="6" Top="2" Right="6" Bottom="2"/>
          <States>
            <Enabled Index="1"/>
            <Focused Index="0"/>
            <Disabled Index="1"/>
          </States>
        </Layer>       
        <Layer Name="Icon" Image="Window.Icon" Width="16" Height="16" OffsetX="0" OffsetY="0">
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////    

    //// Window.Shadow /////////////////////////////////////////////////////////
    <Control Name="Window.Shadow">
      <OriginMargins Left="20" Top="20" Right="20" Bottom="20"/>
      <Layers>
        <Layer Name="Control" Image="Window.Shadow" Width="128" Height="128">
          <SizingMargins Left="40" Top="40" Right="40" Bottom="40"/>
        </Layer>
      </Layers>
    </Control>
    ///////////////////////////////////////////////////////////////////////////

    //// Window.CloseButton ////////////////////////////////////////////////////
    <Control Name="Window.CloseButton" Inherits="Button">
      <DefaultSize Width="42" Height="19"/>            
      <OriginMargins Left="20" Top="20" Right="20" Bottom="25"/>
      <Layers>
        <Layer Name="Control" Image="Window.CloseButton" Width="82" Height="64" OffsetX="-5" OffsetY="0">
          <States>
            <Enabled Index="0" Overlay="false"/>
            <Hovered Index="1" Overlay="false"/>
            <Pressed Index="2" Overlay="false"/>
            <Focused Index="3" Overlay="false"/>
            <Disabled Index="4" Overlay="false"/>
          </States>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// TextBox////////////////////////////////////////////////////////////////
    <Control Name="TextBox" Inherits="Control">
      <DefaultSize Width="128" Height="20"/>
      <ClientMargins Left="4" Top="4" Right="4" Bottom="4"/>
      <Layers>
        <Layer Name="Control" Image="TextBox" Width="8" Height="8">
          <SizingMargins Left="1" Top="1" Right="1" Bottom="1"/>          
          <States>
            <Enabled Index="0"/>
            <Hovered Index="1"/>
            <Pressed Index="2"/>
            <Focused Index="3"/>
            <Disabled Index="4"/>
          </States>
          <Text Font="Default" Alignment="TopLeft" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="32;32;32;255"/>
              <Focused Color="32;32;32;255"/>
              <Pressed Color="32;32;32;255"/>
              <Hovered Color="32;32;32;255"/>
              <Disabled Color="32;32;32;200"/>
            </Colors>
          </Text>
        </Layer>
        <Layer Name="Cursor" Image="TextBox.Cursor" Width="1" Height="1">        
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////   

    //// ListBox ///////////////////////////////////////////////////////////////
    <Control Name="ListBox" Inherits="Control">
      <Layers>
        <Layer Name="Control" Image="ListBox" Width="8" Height="8">
          <ContentMargins Left="2" Top="2" Right="2" Bottom="2"/>
          <SizingMargins Left="1" Top="1" Right="1" Bottom="1"/>
          <States>
            <Enabled Index="0"/>
            <Hovered Index="1"/>
            <Pressed Index="2"/>
            <Focused Index="3"/>
            <Disabled Index="4"/>
          </States>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="2" OffsetY="0">
            <Colors>
              <Enabled Color="32;32;32;255"/>
              <Hovered Color="32;32;32;255"/>
              <Focused Color="32;32;32;255"/>
              <Pressed Color="32;32;32;255"/>
              <Disabled Color="32;32;32;255"/>
            </Colors>
          </Text>
        </Layer>
        <Layer Name="ListBox.Selection" Image="ListBox.Selection" Width="128" Height="16">
          <States>
            <Enabled Index="0" Color="255;255;255;128"/>
            <Hovered Index="-1" Color="255;255;255;255"/>
            <Pressed Index="0" Color="255;255;255;255"/>
            <Focused Index="0" Color="255;255;255;255"/>
            <Disabled Index="0" Color="255;255;255;128"/>
          </States>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="2" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;128"/>
              <Hovered Color="255;255;255;192"/>
              <Focused Color="255;255;255;192"/>
              <Pressed Color="255;255;255;192"/>
              <Disabled Color="128;128;128;128"/>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ComboBox //////////////////////////////////////////////////////////////
    <Control Name="ComboBox" Inherits="TextBox">
      <Layers>
        <Layer Name="Control" Image="ComboBox" Width="8" Height="8">
          <ContentMargins Left="2" Top="2" Right="2" Bottom="2"></ContentMargins>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ComboBox.Button ///////////////////////////////////////////////////////
    <Control Name="ComboBox.Button" Inherits="Button">
      <Layers>
        <Layer Name="Control" Image="ComboBox.Button" Width="16" Height="16">
          <SizingMargins Left="2" Top="2" Right="2" Bottom="2"></SizingMargins>
          <ContentMargins Left="2" Top="2" Right="2" Bottom="2"></ContentMargins>
          <States>
            <Enabled Index="0" Overlay="false"></Enabled>
            <Hovered Index="1" Overlay="false"></Hovered>
            <Pressed Index="2" Overlay="false"></Pressed>
            <Focused Index="3" Overlay="false"></Focused>
            <Disabled Index="4" Overlay="false"></Disabled>
          </States>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ComboBox.ListBox //////////////////////////////////////////////////////////
    <Control Name="ComboBox.ListBox" Inherits="ListBox">
      <OriginMargins Left="0" Top="0" Right="8" Bottom="8"/>
      <Layers>
        <ContentMargins Left="2" Top="2" Right="2" Bottom="2"/>
        <Layer Name="Control" Image="ComboBox.ListBox" Width="32" Height="32">
          <SizingMargins Left="1" Top="1" Right="9" Bottom="9"/>         
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// Panel /////////////////////////////////////////////////////////////////
    <Control Name="Panel" Inherits="Control">
      <Layers>
        <Layer Name="Control" Image="Control" Width="1" Height="1">
          <States>
            <Enabled Index="0" Color="16;16;16;255"></Enabled>
            <Pressed Index="0" Color="16;16;16;255"></Pressed>
            <Hovered Index="0" Color="16;16;16;255"></Hovered>
            <Focused Index="0" Color="16;16;16;255"></Focused>
            <Disabled Index="0" Color="16;16;16;255"></Disabled>
          </States>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ContextMenu ///////////////////////////////////////////////////////////
    <Control Name="ContextMenu" Inherits="Control">
      <OriginMargins Left="0" Top="0" Right="10" Bottom="10"></OriginMargins>     
      <Layers>
        <Layer Name="Control" Image="ContextMenu" Width="64" Height="64">
          <SizingMargins Left="22" Top="2" Right="10" Bottom="10"></SizingMargins>
          <ContentMargins Left="2" Top="2" Right="10" Bottom="10"></ContentMargins>
          <States>
            <Enabled Index="0"></Enabled>
            <Hovered Index="0"></Hovered>
            <Pressed Index="0"></Pressed>
            <Focused Index="0"></Focused>
            <Disabled Index="0"></Disabled>
          </States>     
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="4" OffsetY="0">
            <Colors>
              <Enabled Color="32;32;32;255"></Enabled>
              <Hovered Color="32;32;32;255"></Hovered>
              <Pressed Color="32;32;32;255"></Pressed>
              <Focused Color="32;32;32;255"></Focused>
              <Disabled Color="32;32;32;128"></Disabled>
            </Colors>
          </Text>
        </Layer>
        <Layer Name="Selection" Image="ContextMenu.Selection" Width="128" Height="16">
          <States>
            <Enabled Index="0" Color="255;255;255;255"></Enabled>
            <Hovered Index="-1" Color="255;255;255;255"></Hovered>
            <Pressed Index="0" Color="255;255;255;255"></Pressed>
            <Focused Index="0" Color="255;255;255;255"></Focused>
            <Disabled Index="0" Color="255;255;255;128"></Disabled>
          </States>
          <Text Font="Default" Alignment="MiddleLeft" OffsetX="4" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;128"></Enabled>
              <Hovered Color="255;255;255;128"></Hovered>
              <Focused Color="255;255;255;128"></Focused>
              <Pressed Color="255;255;255;128"></Pressed>
              <Disabled Color="128;128;128;128"></Disabled>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// MainMenu //////////////////////////////////////////////////////////////
    <Control Name="MainMenu" Inherits="Control">      
      <Layers>
        <Layer Name="Control" Image="MainMenu" Width="32" Height="16">
          <SizingMargins Left="0" Top="0" Right="0" Bottom="1"></SizingMargins>
          <ContentMargins Left="2" Top="2" Right="16" Bottom="3"></ContentMargins>
          <States>
            <Enabled Index="0"></Enabled>
            <Hovered Index="0"></Hovered>
            <Pressed Index="0"></Pressed>
            <Focused Index="0"></Focused>
            <Disabled Index="0"></Disabled>
          </States>
          <Text Font="Default" Alignment="MiddleCenter" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="32;32;32;255"></Enabled>
              <Disabled Color="32;32;32;128"></Disabled>
            </Colors>
          </Text>
        </Layer>
        <Layer Name="Selection" Image="MainMenu.Selection" Width="64" Height="16">
          <SizingMargins Left="1" Top="1" Right="1" Bottom="1"/>
          <States>
            <Enabled Index="0" Color="255;255;255;255"></Enabled>
            <Hovered Index="0" Color="255;255;255;255"></Hovered>
            <Pressed Index="0" Color="255;255;255;255"></Pressed>
            <Focused Index="0" Color="255;255;255;255"></Focused>
            <Disabled Index="0" Color="255;255;255;128"></Disabled>
          </States>
          <Text Font="Default" Alignment="MiddleCenter" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;128"></Enabled>
              <Hovered Color="255;255;255;128"></Hovered>
              <Focused Color="255;255;255;128"></Focused>
              <Pressed Color="255;255;255;128"></Pressed>
              <Disabled Color="128;128;128;128"></Disabled>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ToolBar ///////////////////////////////////////////////////////////////
    <Control Name="ToolBar" Inherits="Control">
      <Layers>
        <Layer Name="Control" Image="ToolBar" Width="24" Height="24">
          <SizingMargins Left="8" Top="2" Right="4" Bottom="2"></SizingMargins>
          <ContentMargins Left="12" Top="2" Right="4" Bottom="2"></ContentMargins>
          <States>
            <Enabled Index="0"></Enabled>
            <Hovered Index="0"></Hovered>
            <Pressed Index="0"></Pressed>
            <Focused Index="0"></Focused>
            <Disabled Index="0"></Disabled>
          </States>
          <Text Font="Default" Alignment="MiddleCenter" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="32;32;32;255"></Enabled>
              <Disabled Color="32;32;32;128"></Disabled>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ToolBarPanel //////////////////////////////////////////////////////////
    <Control Name="ToolBarPanel" Inherits="Control">
      <Layers>
        <Layer Name="Control" Image="ToolBarPanel" Width="24" Height="24">
          <SizingMargins Left="0" Top="0" Right="0" Bottom="1"></SizingMargins>
          <ContentMargins Left="0" Top="0" Right="0" Bottom="1"></ContentMargins>
          <States>
            <Enabled Index="0"/>
            <Hovered Index="0"/>
            <Pressed Index="0"/>
            <Focused Index="0"/>
            <Disabled Index="0"/>
          </States>         
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ToolBarButton /////////////////////////////////////////////////////////
    <Control Name="ToolBarButton" Inherits="Control">
      <DefaultSize Width="22" Height="22"/>
      <Layers>
        <Layer Name="Control" Image="ToolBarButton" Width="24" Height="24">
          <SizingMargins Left="1" Top="1" Right="1" Bottom="1"/>
          <ContentMargins Left="3" Top="3" Right="3" Bottom="3"/>
          <States>
            <Enabled Index="0" Color="255;255;255;255"/>
            <Hovered Index="1" Color="255;255;255;255"/>
            <Pressed Index="2" Color="255;255;255;255"/>
            <Focused Index="1" Color="255;255;255;255"/>
            <Disabled Index="0" Color="255;255;255;255"/>
          </States>
          <Text Font="Default" Alignment="MiddleCenter" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="255;255;255;192"/>
              <Focused Color="255;255;255;192"/>
              <Pressed Color="255;255;255;192"/>
              <Hovered Color="255;255;255;240"/>
              <Disabled Color="128;128;128;192"/>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// StatusBar /////////////////////////////////////////////////////////////
    <Control Name="StatusBar" Inherits="Control">
      <Layers>
        <Layer Name="Control" Image="StatusBar" Width="24" Height="24">
          <SizingMargins Left="8" Top="4" Right="4" Bottom="4"></SizingMargins>
          <ContentMargins Left="12" Top="2" Right="4" Bottom="2"></ContentMargins>
          <States>
            <Enabled Index="0"></Enabled>
            <Hovered Index="0"></Hovered>
            <Pressed Index="0"></Pressed>
            <Focused Index="0"></Focused>
            <Disabled Index="0"></Disabled>
          </States>
          <Text Font="Default" Alignment="MiddleCenter" OffsetX="0" OffsetY="0">
            <Colors>
              <Enabled Color="32;32;32;255"></Enabled>
              <Disabled Color="32;32;32;128"></Disabled>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// ToolTip ///////////////////////////////////////////////////////////////
    <Control Name="ToolTip" Inherits="Control">
      <Layers>
        <Layer Name="Control" Image="ToolTip" Width="32" Height="32">
          <SizingMargins Left="5" Top="1" Right="9" Bottom="9"/>
          <ContentMargins Left="9" Top="3" Right="13" Bottom="10"/>
          <States>
            <Enabled Index="0" Color="255;255;255;255"/>
          </States>
          <Text Font="Default" Alignment="MiddleLeft">
            <Colors>
              <Enabled Color="0;0;0;255"/>
            </Colors>
          </Text>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// Bevel /////////////////////////////////////////////////////////////////
    <Control Name="Bevel" Inherits="Control">
      <Layers>
        <Layer Name="Control" Image="Control" Width="1" Height="1">
          <Attributes>
            <Attribute Name="LightColor" Value="80;80;80"></Attribute>
            <Attribute Name="DarkColor" Value="32;32;32"></Attribute>
            <Attribute Name="FlatColor" Value="16;16;16"></Attribute>
          </Attributes>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////      

    //// Dialog ////////////////////////////////////////////////////////////////
    <Control Name="Dialog">
      <Layers>
        <Layer Name="TopPanel">
          <Attributes>
            <Attribute Name="Color" Value="16;16;16"></Attribute>
            <Attribute Name="BevelStyle" Value="Flat"></Attribute>
            <Attribute Name="BevelMargin" Value="1"></Attribute>
            <Attribute Name="CaptFontColor" Value="200;200;200"></Attribute>
            <Attribute Name="CaptFont" Value="Default"></Attribute>
            <Attribute Name="DescFontColor" Value="200;200;200"></Attribute>
            <Attribute Name="DescFont" Value="Default"></Attribute>
          </Attributes>
        </Layer>
        <Layer Name="BottomPanel">
          <Attributes>
            <Attribute Name="Color" Value="16;16;16"></Attribute>
            <Attribute Name="BevelStyle" Value="Flat"></Attribute>
            <Attribute Name="BevelMargin" Value="1"></Attribute>
          </Attributes>
        </Layer>
      </Layers>
    </Control>
    ////////////////////////////////////////////////////////////////////////////

    //// GroupBox //////////////////////////////////////////////////////////////
    <Control Name="GroupBox" Inherits="Control">
      <ClientMargins Left="2" Top="2" Right="2" Bottom="2"></ClientMargins>
      <Layers>
        <Layer Name="Control" Image="GroupBox" Width="8" Height="8">
          <SizingMargins Left="2" Top="2" Right="2" Bottom="2"></SizingMargins>
          <ContentMargins Left="2" Top="2" Right="2" Bottom="2"></ContentMargins>
          <States>
            <Enabled Index="0"></Enabled>
            <Hovered Index="0"></Hovered>
            <Pressed Index="0"></Pressed>
            <Focused Index="0"></Focused>
            <Disabled Index="0"></Disabled>
